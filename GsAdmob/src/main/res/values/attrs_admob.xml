<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerGsAdView">
        <attr name="adsShowType" format="enum">
            <enum name="showIfSuccess" value="0"/>
            <enum name="alwaysShow" value="1"/>
            <enum name="hide" value="2"/>
            <enum name="notShow" value="3"/>
        </attr>
        <attr name="adsBannerGsBackgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="BaseNativeAdView">
        <attr name="adsNativeMode" format="enum">
            <enum name="none" value="0"/>
            <enum name="album" value="1"/>
            <enum name="font" value="2"/>
            <enum name="frame" value="3"/>
            <enum name="language" value="4"/>
            <enum name="share" value="5"/>
            <enum name="sticker" value="6"/>
            <enum name="template" value="7"/>
            <enum name="vip" value="8"/>
            <enum name="custom" value="9"/>
        </attr>
        <attr name="adsLayoutId" format="reference"/>
        <attr name="adsLayoutShimmerId" format="reference"/>
        <attr name="adsHeadlineId" format="reference"/>
        <attr name="adsBodyId" format="reference"/>
        <attr name="adsStarsId" format="reference"/>
        <attr name="adsAppIconId" format="reference"/>
        <attr name="adsCallToActionId" format="reference"/>
        <attr name="adsViewId" format="reference"/>
        <attr name="adsMediaViewId" format="reference"/>
        <attr name="adsShimmerId" format="reference"/>
        <attr name="adsNativeViewRoot" format="reference"/>
    </declare-styleable>
</resources>